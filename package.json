{
  "name": "@hoijnet/tuscli",
  "version": "0.2.3",
  "description": "TerminusDB Javascript CLI",
  "main": "dist/tuscli.js",
  "bin": {
    "tuscli": "./dist/tuscli.js"
  },
  "scripts": {
    "start": "node dist/tuscli.js",
    "build": "npx tsc --noEmit false --module commonjs",
    "buildexe": "npx tsc --noEmit false --module commonjs && mkdir -p bin && npx pkg -t node14-linuxstatic-x64 dist/tuscli.js -o bin/tuscli",
    "buildmac": "npx tsc --noEmit false --module commonjs && mkdir -p bin && npx pkg dist/tuscli.js -o bin/tuscli-mac",
    "dockerize": "docker build --pull -t hoijnet/tuscli -f docker/Dockerfile .",
    "dockerpush": "docker push hoijnet/tuscli",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hoijnet/tuscli.git"
  },
  "author": "Philippe HÃ¶ij <philippe@hoij.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hoijnet/tuscli/issues"
  },
  "homepage": "https://github.com/hoijnet/tuscli#readme",
  "dependencies": {
    "@terminusdb/terminusdb-client": "^10.0.32",
    "commander": "^8.3.0",
    "debug": "^4.3.3",
    "json-colorizer": "^2.2.2",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/node": "^16.11.21",
    "pkg": "^5.8.0",
    "typescript": "^4.5.5"
  }
}
