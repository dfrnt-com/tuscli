{
  "name": "@hoijnet/tuscli",
  "version": "0.0.9",
  "description": "TerminusDB Javascript CLI",
  "main": "dist/tuscli.js",
  "bin": {
    "tuscli": "./dist/tuscli.js"
  },
  "scripts": {
    "start": "node dist/tuscli.js",
    "build": "npx tsc --noEmit false --module commonjs",
    "buildnexe": "npx tsc --noEmit false --module commonjs && npx nexe --build -o tuscli",
    "buildnexe-docker": "npx tsc --noEmit false --module commonjs && npx nexe -o tuscli",
    "dockerize": "docker build --pull -t hoijnet/tuscli -f docker/Dockerfile .",
    "dockerpush": "docker push hoijnet/tuscli",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hoijnet/tuscli.git"
  },
  "author": "Philippe HÃ¶ij <philippe@hoij.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hoijnet/tuscli/issues"
  },
  "homepage": "https://github.com/hoijnet/tuscli#readme",
  "dependencies": {
    "@terminusdb/terminusdb-client": "10.0.2",
    "commander": "^8.3.0",
    "debug": "^4.3.3",
    "json-colorizer": "^2.2.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/node": "^16.11.19",
    "nexe": "^4.0.0-beta.19",
    "typescript": "^4.5.4"
  }
}
